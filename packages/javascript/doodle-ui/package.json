    "type": "module",
    "scripts": {
        "dev": "yarn storybook",
        "build": "tsc && vite build && yarn build:styles",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "test": "TZ=America/Los_Angeles vitest",
        "prepack": "pinst --disable",
        "postpack": "pinst --enable",
        "postinstall": "husky",
        "preview": "vite preview",
        "storybook": "storybook dev -p 6006",
        "build:storybook": "storybook build",
        "build:styles": "npx tailwindcss -i ./src/input.css -o ./dist/styles.css --minify",
        "create-component": "tsx ./scripts/createComponent.ts",
        "format:check": "npx prettier . --check",
        "format:write": "npx prettier . --write",
        "update-badge": "tsx ./scripts/updateVersionBadge.ts $npm_package_version $PROJECT_CWD"
    },
