{
    "name": "bloodhound-ui",
    "version": "0.0.0",
    "type": "module",
    "private": true,
    "scripts": {
        "debug": "vite --debug",
        "dev": "vite",
        "start": "vite",
        "build": "yarn check-types && vite build",
        "preview": "vite preview",
        "test": "TZ=America/Los_Angeles vitest",
        "check-types": "yarn workspaces foreach -ptRv --include '{bh-shared-ui,js-client-library}' run build && tsc --noEmit --pretty",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "format": "prettier --write \"src/**/*.@(js|jsx|ts|tsx|md|html|css|scss|json)\"",
        "check-format": "prettier  --list-different \"src/**/*.@(js|jsx|ts|tsx|md|html|css|scss|json)\""
    },
    "dependencies": {
        "@date-io/luxon": "^1.3.13",
        "@emotion/react": "^11.10.4",
        "@emotion/styled": "^11.10.4",
        "@fontsource/roboto": "^4.5.1",
        "@fontsource/roboto-mono": "^4.5.0",
        "@fortawesome/fontawesome-free": "^6.4.2",
        "@fortawesome/fontawesome-svg-core": "^6.4.2",
        "@fortawesome/free-solid-svg-icons": "^6.4.2",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@mui/lab": "^5.0.0-alpha.136",
        "@mui/material": "^5.10.8",
        "@mui/styles": "^5.14.0",
        "@mui/x-date-pickers": "^5.0.20",
        "@neo4j-cypher/editor-support": "^1.0.1",
        "@neo4j-cypher/react-codemirror": "^1.0.3",
        "@react-sigma/core": "^3.4.1",
        "@reduxjs/toolkit": "^1.8.5",
        "bh-shared-ui": "workspace:*",
        "dagrejs": "git@github.com:BloodHoundAD/dagre.git",
        "downshift": "^6.1.7",
        "graphology": "^0.25.1",
        "graphology-layout": "^0.6.1",
        "graphology-layout-forceatlas2": "^0.10.1",
        "history": "5.3.0",
        "immer": "^10.0.2",
        "js-client-library": "workspace:*",
        "js-cookie": "^3.0.1",
        "js-file-download": "^0.4.12",
        "lodash": "^4.17.21",
        "luxon": "^2.0.2",
        "memoize-one": "^6.0.0",
        "notistack": "^2.0.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-error-boundary": "^3.1.4",
        "react-hook-form": "^7.0.0",
        "react-query": "^3.39.3",
        "react-redux": "^8.1.1",
        "react-router-dom": "6.4.1",
        "react-window": "^1.8.9",
        "redux-saga": "^1.1.3",
        "sass": "^1.62.1",
        "sigma": "^2.4.0",
        "swagger-ui-react": "^4.15.2"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.3.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/js-cookie": "^3.0.3",
        "@types/lodash": "^4.14.195",
        "@types/luxon": "^2.4.0",
        "@types/node": "^18.16.19",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@types/react-router-dom": "^5.3.3",
        "@types/react-window": "^1.8.5",
        "@types/swagger-ui-react": "^4.18.0",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "@vitejs/plugin-react": "^4.0.3",
        "@vitest/coverage-v8": "^0.32.0",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.3.5",
        "graphology-types": "^0.24.7",
        "jest-webgl-canvas-mock": "^2.5.0",
        "jsdom": "^20.0.2",
        "lint-staged": "^10.5.4",
        "msw": "^1.2.1",
        "prettier": "^2.8.8",
        "redux-devtools-extension": "^2.13.9",
        "redux-saga-test-plan": "^4.0.3",
        "typescript": "^5.1.6",
        "vite": "^4.4.4",
        "vite-plugin-glsl": "^1.1.2",
        "vitest": "^0.32.0",
        "vitest-canvas-mock": "^0.3.2",
        "whatwg-fetch": "^3.6.2"
    },
    "packageManager": "yarn@3.5.0",
    "msw": {
        "workerDirectory": "public"
    },
    "installConfig": {
        "hoistingLimits": "workspaces"
    }
}
